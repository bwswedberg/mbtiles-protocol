{
  "name": "vector-tile-protocols",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "start": "DOTENV_CONFIG_PATH=./.env.local NODE_OPTIONS='-r dotenv/config' webpack serve --mode development",
    "build": "rm -rf dist && DOTENV_CONFIG_PATH=./.env.prod NODE_OPTIONS='-r dotenv/config' webpack --mode production",
    "deploy": "rm -rf docs && cp -R dist docs",
    "format:geojson": "ts-node ./scripts/formatGeolifeAsGeojson.ts limit=1000000 output=./public/geolife-1mv2.geojson",
    "tippecanoe:exec": "tippecanoe -f -P -z12 -r1 -l geolife --cluster-distance=10 -o geolife-full-z12cd10.mbtiles data/geolife-full.geojson",
    "sqlite:dumpmbtiles": "sqlite3 public/geolife-full-v12.mbtiles \"SELECT writefile('public/geolife-full-v12' || '/' || zoom_level || '/' || tile_column || '/' || tile_row || '.pbf.gz', tile_data) FROM tiles\"",
    "sqlite:zip": "zip -0 -r public/geolife-full-v12.zip public/geolife-full-v12"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "fflate": "^0.7.3",
    "idb": "^7.0.2",
    "mapbox-gl": "^2.8.2",
    "maplibre-gl": "^2.1.9",
    "pmtiles": "^1.1.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-map-gl": "^7.0.15",
    "react-router-dom": "^6.3.0",
    "sql.js": "^1.7.0"
  },
