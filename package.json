    "@babel/preset-typescript": "^7.17.12",
    "@babel/runtime": "^7.18.3",
    "@mapbox/mbtiles": "^0.12.1",
    "@types/fork-ts-checker-webpack-plugin": "^0.4.5",
    "@types/glob": "^7.2.0",
    "@types/node": "^17.0.42",
    "@types/pako": "^2.0.0",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "@types/sql.js": "^1.4.3",
